	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

-- Tabs
  Tab:AddToggle({
	Name = "Auto Damage",
	Default = false,
	Callback = function(Value)
	  getgenv().SwordSimulator.AutoDamage = Value
	  if Value then AutoDamage() end
	end    
})

Tab:AddToggle({
	Name = "Kill Aura",
	Default = false,
	Callback = function(Value)
