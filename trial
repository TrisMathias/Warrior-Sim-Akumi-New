getgenv().SwordSimulator = {
    AutoDamage = false,
    KillAura = false,
    ClaimGift = false,
    GetCodes = false,
    MazeGem = false,
    RaidGem = false,
    W1L1 = false,
};

--XP FARM
function W1L1()
    local xpDummies = {
        Vector3.new(-1012.8, 3.9, -4818.8),
        Vector3.new(-2216.7, 3.9, -4818.8),
        Vector3.new(-3452.5, 8.0, -4818.9)
    }
    
    for _, position in ipairs(xpDummies) do
        -- Replace the following line with your game-specific logic for interacting with the XP dummies
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Skill"):WaitForChild("ServerBehaviour"):FireServer(true, position)
        wait(0.5)
    end
end

--XP FARM TOGGLE
WORLDSTab.newToggle("LVL1 XP DUMMY", "AUTO XP FARM (W1 L1)", false, function(toggleState)
    getgenv().SwordSimulator.W1L1 = toggleState
    if toggleState then
        spawn(function()  -- Run in a separate thread
            while getgenv().SwordSimulator.W1L1 do
                W1L1()
                wait(0.1)  -- Check periodically
            end
        end)
    end
end)

end
