-- BACKROOMS AKUMI SPECIAL SCRIPT

local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("BACKROOMS AKUMI SPECIAL SCRIPT", "Default")
if game.PlaceId == 17856546976 then

    -- TABS
    local FarmTab = DrRayLibrary.newTab("WAVE FARM", "ImageIdHere")
    local MISCTab = DrRayLibrary.newTab("EXTRA", "ImageIdHere")
    local WORLDSTab = DrRayLibrary.newTab("WORLDS TP", "ImageIdHere")

    -- TOGGLES/BUTTONS
    FarmTab.newToggle("AUTO DAMAGE", "AUTO ATTACK (NO ANIMATION)", false, function(toggleState)
        getgenv().SwordSimulator.AutoDamage = toggleState
        if toggleState then
            AutoDamage()
        end
    end)

    FarmTab.newToggle("KILL AURA", "Enemy One Hit! (Kill Doesn't Count)", false, function(toggleState)
        getgenv().SwordSimulator.KillAura = toggleState
        if toggleState then
            KillAura()
        end
    end)

    MISCTab.newToggle("CLAIM GIFT", "Claims Daily Gift (BYPASS WAIT TIME)", false, function(toggleState)
        getgenv().SwordSimulator.ClaimGift = toggleState
        if toggleState then
            ClaimGift()
        end
    end)

    MISCTab.newToggle("GET CODES", "GET GAME CODES", false, function(toggleState)
        getgenv().SwordSimulator.GetCodes = toggleState
        if toggleState then
            GetCodes()
        end
    end)

    MISCTab.newToggle("MAZE GEMS", "GET MAZE GEMS", false, function(toggleState)
        getgenv().SwordSimulator.MazeGem = toggleState
        if toggleState then
            MazeGem()
        end
    end)

    MISCTab.newToggle("Raid Gems", "Buys Raid Shop Gems (Using Tokens)", false, function(toggleState)
        getgenv().SwordSimulator.RaidGem = toggleState
        if toggleState then
            RaidGem()
        end
    end)

    WORLDSTab.newButton("FOREST (W1)", "TELEPORTS TO FOREST", function()
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Common"):WaitForChild("Door"):WaitForChild("Teleport"):FireServer(1)
    end)

    WORLDSTab.newButton("DESERT (W2)", "TELEPORTS TO DESERT", function()
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Common"):WaitForChild("Door"):WaitForChild("Teleport"):FireServer(2)
    end)

    WORLDSTab.newButton("BEACH (W3)", "TELEPORTS TO BEACH", function()
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Common"):WaitForChild("Door"):WaitForChild("Teleport"):FireServer(3)
    end)

    WORLDSTab.newButton("SNOW (W4)", "TELEPORTS TO SNOW", function()
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Common"):WaitForChild("Door"):WaitForChild("Teleport"):FireServer(4)
    end)

    WORLDSTab.newButton("VOLCANO (W5)", "TELEPORTS TO VOLCANO", function()
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Common"):WaitForChild("Door"):WaitForChild("Teleport"):FireServer(5)
    end)

    WORLDSTab.newButton("CASTLE (W6)", "TELEPORTS TO CASTLE", function()
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Common"):WaitForChild("Door"):WaitForChild("Teleport"):FireServer(6)
    end)

    WORLDSTab.newButton("CANDY (W7)", "TELEPORTS TO CANDY", function()
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Common"):WaitForChild("Door"):WaitForChild("Teleport"):FireServer(7)
    end)

    WORLDSTab.newButton("GARDEN (W8)", "TELEPORTS TO GARDEN", function()
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Common"):WaitForChild("Door"):WaitForChild("Teleport"):FireServer(8)
    end)

    -- VALUES
    getgenv().SwordSimulator = {
        AutoDamage = false,
        KillAura = false,
        ClaimGift = false,
        GetCodes = false,
        MazeGem = false,
        RaidGem = false,
    }

    -- FUNCTIONS

    -- AUTO DAMAGE
    function AutoDamage()
        while getgenv().SwordSimulator.AutoDamage do
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Weapon"):WaitForChild("TakeDamage"):FireServer()
            wait(0.001)
        end
    end

    -- KILL AURA
    function KillAura()
        while getgenv().SwordSimulator.KillAura do
            while wait(15) do
                pcall(function()
                    sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", 112412400000)
                    sethiddenproperty(game.Players.LocalPlayer, "MaxSimulationRadius", 112412400000)
                    for i, d in pairs(game:GetService("Workspace"):GetDescendants()) do
                        if d.ClassName == 'Humanoid' and d.Parent.Name ~= game.Players.LocalPlayer.Name then
                            d.Health = 0
                        end
                    end
                end)
            end
        end
    end

    -- CLAIM GIFT
    function ClaimGift()
        while getgenv().SwordSimulator.ClaimGift do
            pcall(function()
                for i = 1, 8 do
                    game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Common"):WaitForChild("Online"):WaitForChild("TryGetOnlineGift"):InvokeServer(i)
                    wait(3)
                end
            end)
        end
    end

    -- GET CODES
    function GetCodes()
        while getgenv().SwordSimulator.GetCodes do
            pcall(function()
                local codes = {"like400k", "welcome", "0.5mlike"}
                for _, code in ipairs(codes) do
                    game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Common"):WaitForChild("Reward"):WaitForChild("CodeReward"):FireServer(code)
                    wait(3)
                end
            end)
        end
    end

    -- MAZE GEM
    function MazeGem()
        while getgenv().SwordSimulator.MazeGem do
            pcall(function()
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("BackRoom"):WaitForChild("CollectChest"):FireServer()
                wait(2)
            end)
        end
    end

    -- RAID GEMS
    function RaidGem()
        while getgenv().SwordSimulator.RaidGem do
            pcall(function()
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Common"):WaitForChild("DailyShop"):WaitForChild("BuyItem"):FireServer(2)
                wait(0.5)
            end)
        end
    end
end

-- FOREST
local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
